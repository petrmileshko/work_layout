    "@babel/preset-env": "7.18.6",
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "10.3.1",
    "babel-loader": "8.2.5",
    "browser-sync": "^2.27.9",
    "critical": "^1.3.10",
    "del": "6.0.0",
    "eslint": "8.19.0",
    "eslint-config-htmlacademy": "6.0.0",
    "gulp": "4.0.2",
    "gulp-htmlmin": "5.0.1",
    "gulp-libsquoosh": "^1.0.9",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "5.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-svgstore": "8.0.0",
    "gulp-terser": "2.0.1",
    "gulp-webp": "4.0.1",
    "pixel-glass": "^1.0.13",
    "postcss": "8.3.6",
    "postcss-csso": "5.0.1",
    "sass": "^1.65.1",
    "stylelint": "13.13.1",
    "stylelint-config-htmlacademy": "0.1.10",
    "webpack": "5.73.0",
    "webpack-stream": "^7.0.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --custom-syntax postcss-scss",
    "lint": "eslint source/js",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp"
